      "features": {
        "search": {
          "enabled": true,
          "search_fields": ["product_name", "description", "category", "producer"],
          "filters": {
            "category": true,
            "price_range": true,
            "quality": true,
            "availability": true,
            "location": true
          },
          "sorting": {
            "price_asc": "Price: Low to High",
            "price_desc": "Price: High to Low",
            "name_asc": "Name: A to Z",
            "name_desc": "Name: Z to A",
            "date_desc": "Newest First",
            "popularity": "Most Popular"
          }
        },
        "cart": {
          "enabled": true,
          "features": ["add_items", "remove_items", "quantity_adjustment", "total_calculation", "item_count"]
        },
        "wishlist": {
          "enabled": true,
          "features": ["add_items", "remove_items", "view_list"]
        }
      },
      
      "payment_methods": [
        {
          "id": "momo",
          "name": "MTN Mobile Money (Momo)",
          "type": "mobile_money",
          "provider": "MTN",
          "requirements": ["phone_number", "pin"],
          "process": [
            "Enter Momo phone number",
            "Enter Momo PIN",
            "Receive USSD prompt on phone",
            "Confirm transaction on phone"
          ],
          "branding": {
            "color": "yellow",
            "icon": "üì±"
          }
        },
        {
          "id": "airtel",
          "name": "Airtel Money",
          "type": "mobile_money",
          "provider": "Airtel",
          "requirements": ["phone_number", "pin"],
          "process": [
            "Enter Airtel Money phone number",
            "Enter Airtel Money PIN",
            "Receive USSD prompt on phone",
            "Confirm transaction on phone"
          ],
          "branding": {
            "color": "red",
            "icon": "üì±"
          }
        }
      ],
      
      "checkout_process": {
        "steps": [
          {
            "step": 1,
            "name": "delivery_details",
            "title": "Delivery Details",
            "fields": [
              {"name": "fullName", "type": "text", "required": true, "placeholder": "Enter your full name"},
              {"name": "phone", "type": "tel", "required": true, "placeholder": "07X XXX XXXX"},
              {"name": "email", "type": "email", "required": false, "placeholder": "Enter your email"},
              {"name": "deliveryAddress", "type": "text", "required": true, "placeholder": "Enter your delivery address"},
              {"name": "deliveryInstructions", "type": "text", "required": false, "placeholder": "Any special delivery instructions"}
            ]
          },
          {
            "step": 2,
            "name": "payment_method",
            "title": "Payment Method",
            "description": "Choose between MTN Momo and Airtel Money"
          },
          {
            "step": 3,
            "name": "processing",
            "title": "Processing Payment",
            "description": "Payment verification and order confirmation"
          },
          {
            "step": 4,
            "name": "success",
            "title": "Payment Successful",
            "description": "Order confirmed and payment received"
          }
        ]
      },
      
      "delivery": {
        "standard_delivery": "2-3 business days",
        "free_delivery_threshold": 10000,
        "free_delivery_currency": "RWF",
        "coverage": "All 29 districts in Rwanda",
        "delivery_instructions": "Can be added during checkout"
      },
      
      "user_roles": {
        "buyers": {
          "permissions": [
            "browse_products",
            "search_and_filter",
            "add_to_cart",
            "manage_wishlist",
            "complete_checkout",
            "view_order_history"
          ]
        },
        "producers_farmers": {
          "permissions": [
            "list_products",
            "manage_inventory",
            "set_pricing",
            "track_sales",
            "manage_orders"
          ]
        },
        "admins": {
          "permissions": [
            "full_backend_access",
            "analytics_dashboard",
            "data_management",
            "user_management",
            "reporting"
          ]
        }
      },
      
      "technical_specs": {
        "framework": "Next.js 15",
        "react_version": "React 19",
        "styling": "Tailwind CSS",
        "theme": "Dark mode with slate colors",
        "responsive": "Mobile-first design",
        "typescript": true
      },
      
